<div class="container">
  <div class="row-fluid">
    <div class="add-thin-line">
    </div>
    <center><div class="profile-page-pic"><%= image_tag(current_user.user_image, :class => "profile-pic") %></center>
    </div>
  </div>
</div>

<div class="container">
  <div class="row-fluid">
    <div class="user-stats">
      <div class="span2"><h4 class="stats"># Lists: <%= @lists.count %></h4></div>
      <div class="span2"><h4 class="stats"># Favorites: <%= @favorites_count %> </h4></div>
      <div class="span2"><h4 class="stats"># Upvotes: <%= @lists.last.favorites.first.influence.tabulate_score %></h4></div>
      <div class="span2"><h4 class="stats"># Relists:</h4></div>
    </div>
  </div>
</div>

<ul> 
  <% @lists.each do |list| %>
    <li> <%= list.title %> </li>
    <% if current_user %>
        <%= link_to "Delete List", user_list_path(current_user, list), :method => :delete %>
        <%= link_to "Edit List", edit_user_list_path(current_user, list) %>
    <% end %>
    <ol>
    <% list.favorites.each do |fav| %>
      <li><%= fav.name %>: <%= fav.description %> </li>
    <% end %>
  </ol>
  <% end %>
</ul>


